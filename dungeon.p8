pico-8 cartridge // http://www.pico-8.com
version 38
__lua__

function init_char(dx,dy,idle,walk)
 return {dx=dx,dy=dy,fr=0,new_dx=dx,new_dy=dy,t=0,f=1,s=4,idle=idle,walk=walk}
end

player = init_char(0,0,{64,80,64},{64,65})
enemy_idle_frames = {96,97,98,99,99,98,97,96}
enemies = {
    init_char(16,8,enemy_idle_frames),
    init_char(32,16,enemy_idle_frames),
    init_char(24,32,enemy_idle_frames),
    init_char(8,64,enemy_idle_frames)
}
in_overworld = true
in_battle = false
transitioning = false

function _init()
end

function _update()
 move_player()
end

function _draw()
 cls()
 if(transitioning) then
  draw_transition()
 elseif(in_overworld) then
  draw_overworld()
 elseif(in_battle) then
  draw_battle()
 end
end

function draw_overworld()
 map(0, 0, 0, 0, 128, 32)
 draw_player()
 for _, e in ipairs(enemies) do
  draw_enemy(e)
 end
end

function draw_player()
 if(is_moving() or (btn(0) or btn(1) or btn(2) or btn(3))) then
  animation(player, player.walk)
 else
  animation(player, player.idle)
 end
end

function draw_enemy(e)
  animation(e, e.idle)
end

function animation(c, frames)
 c.t=(c.t+1)%c.s
 if(c.t==0) c.f= c.f%#frames+1
 spr(frames[c.f], c.dx, c.dy)
end

function move_player()
 if(not in_overworld) return
 old_x = player.dx
 old_y = player.dy
 
 if(not is_moving()) then
  if (btn(0)) then
   player.new_dx -= 8
  elseif (btn(1)) then
   player.new_dx += 8
  elseif (btn(2)) then
   player.new_dy -= 8
  elseif (btn(3)) then
   player.new_dy += 8
  end
 end

 if(not fget(mget(player.new_dx/8,player.new_dy/8),0)) then
  player.new_dx = old_x
  player.new_dy = old_y
 end

 if(player.new_dx > player.dx) player.dx +=1
 if(player.new_dx < player.dx) player.dx -=1
 if(player.new_dy > player.dy) player.dy +=1
 if(player.new_dy < player.dy) player.dy -=1

 if(is_encountered()) then
  transitioning = true
 end
end

function is_encountered()
 for _, e in ipairs(enemies) do
  if(player.dx == e.dx and player.dy == e.dy) then
   transitioning = true
  end
 end
end

function is_moving()
 return player.new_dx != player.dx or player.new_dy != player.dy
end

function draw_battle(players,enemies)
 draw_action_menu()
 draw_fighters()
end

function draw_action_menu()
 rect(70,96,127,127,6)
 color(7)
 print('attack',85,100)
 print('magic',85,110)
 print('abilities',85,120)
 print('➡️',75,100)
end

function draw_fighters()
 color(6)
 rect(1,1,120,20)
 print('armor      aura      hp',5,5)
 print('1500       600       3000',5,13)
end

function draw_transition()
 if(transitioning) then
  if(height == nil) height = 1
  rectfill(1,1,127,height,1)

  height += 4
  if(height >= 128) then
   height = nil
   transitioning = false
   toggle_battle()
  end
 end
end

function toggle_battle() 
 if(in_overworld and not in_battle) then
  in_overworld = false
  in_battle = true
 else
  in_overworld = true
  in_battle = false
 end
end

